<div class="form-group">
  <label>{{localize "PF1KS.SettlementLabel"}}</label>
  <div class="form-fields">
    <select name="system.settlementId">
      {{selectOptions settlementIdChoices selected=system.settlementId}}
    </select>
  </div>
</div>
<div class="form-group">
  <label>{{localize "PF1KS.Type"}}</label>
  <div class="form-fields">
    <select class="sub-type-selector" name="system.subType">
      {{selectOptions subTypeChoices selected=system.subType}}
    </select>
  </div>
</div>

<h3 class="form-header">{{localize "PF1KS.Changes"}}</h3>
{{> item-sheet-changes}}
